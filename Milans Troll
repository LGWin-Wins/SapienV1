coroutine.wrap(PJZT_fake_script)()
local function TKQOYEB_fake_script() -- TextButton.Something 
	local script = Instance.new('LocalScript', TextButton)

	local players = game:GetService("Players")
	local localplr = players.LocalPlayer
	
	local function teleportToPlayer(partialName)
		for _, player in pairs(players:GetPlayers()) do
			if player ~= localplr and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				if string.find(string.lower(player.Name), string.lower(partialName)) then
					localplr.Character.HumanoidRootPart.Position = player.Character.HumanoidRootPart.Position
					return
				end
			end
		end
	end
	
	script.Parent.MouseButton1Up:Connect(function()
		teleportToPlayer(script.Parent.Parent.TextBox.Text)
	end)
end
