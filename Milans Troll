local function XSXFD_fake_script() -- BaseplateChanger.LocalScript 
	local script = Instance.new('LocalScript', BaseplateChanger)

	loadstring(game:HttpGet("https://raw.githubusercontent.com/LGWin-Wins/Milan-s-Library/refs/heads/main/Baseplate%20Changer"))()
	local button = script.Parent  -- The TextButton itself
	local content = game.Workspace.Content  -- The object you want to show/hide (e.g., a Frame or any other object)
	
	-- Function to toggle the visibility of the content
	local function toggleContent()
		content.Visible = not content.Visible
	end
	
	-- Connect the function to the button's MouseButton1Click event
	button.MouseButton1Click:Connect(toggleContent)
	
end
