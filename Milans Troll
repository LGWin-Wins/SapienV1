-- Scripts:

local function AGKGWB_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local Frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = false
	end)
end
coroutine.wrap(AGKGWB_fake_script)()
local function HFKX_fake_script() -- title.Rainbow Script 
	local script = Instance.new('LocalScript', title)

	local textLabel = script.Parent -- Get the TextLabel
	local hue = 0 -- Start color at 0 (Red in HSV)
	
	while true do
		hue = (hue + 1) % 360 -- Cycle through colors smoothly
		textLabel.TextColor3 = Color3.fromHSV(hue / 360, 1, 1) -- Convert to RGB
		wait(0.05) -- Adjust speed (Lower = Faster)
	end
	
end
coroutine.wrap(HFKX_fake_script)()
local function QRBEOE_fake_script() -- flyv3.rainbow 
	local script = Instance.new('LocalScript', flyv3)

	local textLabel = script.Parent -- Get the TextLabel
	local hue = 0 -- Start color at 0 (Red in HSV)
	
	while true do
		hue = (hue + 1) % 360 -- Cycle through colors smoothly
		textLabel.TextColor3 = Color3.fromHSV(hue / 360, 1, 1) -- Convert to RGB
		wait(0.05) -- Adjust speed (Lower = Faster)
	end
	
end
coroutine.wrap(QRBEOE_fake_script)()
